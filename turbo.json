{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "build/**"
      ]
    },
    "publish": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "README.md",
        "package.json"
      ],
      "outputs": [
        "build/**"
      ]
    }
  }
}
